syntax = "proto3";

package example;

import "google/protobuf/timestamp.proto";

message Person {
  string name = 1;
  int32 id = 2;
  string email = 3;

  enum PhoneType {
    MOBILE = 0;
    HOME = 1;
    WORK = 2;
  }

  message PhoneNumber {
    string number = 1;
    PhoneType type = 2;
  }

  repeated PhoneNumber phones = 4;

  google.protobuf.Timestamp last_updated = 5;

  oneof test_oneof {
    string test_string = 6;
    bool test_bool = 7;
  }

  map<string, string> attributes = 8;

  reserved 9, 10, 11 to 15;
  reserved "foo", "bar";
}
